   "source": [
    "from collections import Counter\n",
    "# use freuquency to know the repetations\n",
    "\n",
    "\n",
    "class Solution:\n",
    "    def firstUniqChar(self, s: str) -> int:\n",
    "        freq = Counter(s)\n",
    "        for char in s:\n",
    "            if freq[char] == 1:\n",
    "                return char\n",
    "        return -1"
   ]
  }
